player1=input("Enter your name:")
colors=input("Guess the color:")
if colors=='Blue':
	print("Wrong")
elif colors=='Green':
	print("Correct")
elif colors=='Violet':
	print("Wrong")
elif colors=='Black':
	print("Wrong")
elif colors=='White':
	print("Wrong")
elif colors=='Brown':
	print("Wrong")
elif colors=='Red':
	print("Wrong")
elif colors=='Orange':
	print("Wrong")
elif colors=='Yellow':
	print("Wrong")
elif colors=='Gray':
	print("Wrong")
else:("Error")
#———————————————————————

